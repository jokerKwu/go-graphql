# GraphQL schema example
#
# https://gqlgen.com/getting-started/
# GraphQL 스키마를 추가할 파일입니다.

type User {
    _id: String
    name: String!
    age: Int!
    phone: String!
}

input NewUser {
    name: String!
    age: Int!
    phone: String!
}

type Query {
    user(_id: String!): User!
    users: [User!]!
}

type Mutation {
    createUser(input: NewUser!): User!
}